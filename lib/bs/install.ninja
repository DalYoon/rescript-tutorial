rescript = 1
rule cp
  command = cp $i $out
rule touch
 command = touch $out
o Index.cmi : cp ../bs/src/Index.cmi
o Index.cmj : cp ../bs/src/Index.cmj
o Index.cmt : cp ../bs/src/Index.cmt
o Index.res : cp ../../src/Index.res
o FizzBuzz.cmi : cp ../bs/src/FizzBuzz.cmi
o FizzBuzz.cmj : cp ../bs/src/FizzBuzz.cmj
o FizzBuzz.cmt : cp ../bs/src/FizzBuzz.cmt
o FizzBuzz.res : cp ../../src/FizzBuzz.res
build install.stamp : touch Index.cmi Index.cmj FizzBuzz.cmi FizzBuzz.cmj 
